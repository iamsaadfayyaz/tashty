<div class="modal fade" id="model-add-rating" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="color-line"></div>
            <div class="modal-header text-center p-xs">
                <h4 class="modal-title">Add Rating</h4>

            </div>
            <div class="modal-body">
                <input id="hdnOrderID" type="hidden">
                <input id="txtRating" class="rating" data-min="0" data-max="5" data-step="1" data-size="xs" data-show-clear="false">
            </div>
            <div class="modal-footer">
                <button type="button" id="btnSaveRating" class="btn btn-success">Save</button>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
  
    $('#btnSaveRating').on('click', function () {

        var orderID = _get("hdnOrderID");
        var rate = _get("txtRating");
        cord.AddRating(orderID, @ViewBag.profileID, rate);
        CloseModel("model-add-rating");
        $('#order-table').DataTable().ajax.reload();
    });


</script>