@if (Model.FoodsCount > 15)
{
    if (Model.Pager.EndPage > 1)
    {
        <div class="pagination-div">
            <ul class="pagination justify-content-center">
                @if (Model.Pager.CurrentPage > 1)
                {
                    <li class="page-item">
                        <button class="page-link" onclick="DiscoverFoodsResults(1)">First</button>
                    </li>
                    <li class="page-item">
                        <button class="page-link" onclick="DiscoverFoodsResults(@(Model.Pager.CurrentPage - 1))">Previous</button>
                    </li>
                }

                @for (var page = Model.Pager.StartPage; page <= Model.Pager.EndPage - 1; page++)
                {
                    <li class="@(page == Model.Pager.CurrentPage ? "active" : "") page-item">
                        <button class="page-link" onclick="DiscoverFoodsResults(@page)">@page</button>
                    </li>
                }

                @if (Model.Pager.CurrentPage < Model.Pager.TotalPages - 1)
                {
                    <li class="page-item">
                        <button class="page-link" onclick="DiscoverFoodsResults(@(Model.Pager.CurrentPage + 1))">Next</button>
                    </li>
                    <li class="page-item">
                        <button class="page-link" onclick="DiscoverFoodsResults(@(Model.Pager.TotalPages))">Last</button>
                    </li>
                }
            </ul>
        </div>
    }
}
<script>
    window._ = new Glider(document.querySelector('.glider'), {
        slidesToShow: 1, //'auto',
        slidesToScroll: 1,
        itemWidth: 150,
        draggable: true,
        scrollLock: false,
        dots: '#dots',
        rewind: true,
        arrows: {
            prev: '.glider-prev',
            next: '.glider-next'
        },
        responsive: [
            {
                // screens greater than >= 775px
                breakpoint: 750,
                settings: {
                    // Set to `auto` and provide item width to adjust to viewport
                    slidesToShow: 'auto',
                    slidesToScroll: 'auto',
                    itemWidth: 275,
                    exactWidth: true,
                    duration: 0.25
                }
            },
            {
                breakpoint: 800,
                settings: {
                    slidesToScroll: 'auto',
                    itemWidth: 275,
                    slidesToShow: 'auto',
                    exactWidth: true
                }
            },
            {
                breakpoint: 700,
                settings: {
                    slidesToShow: 'auto',
                    slidesToScroll: 'auto',
                    itemWidth: 275,
                    exactWidth: true,
                    duration: 0.25
                }
            },
            {
                breakpoint: 600,
                settings: {
                    slidesToScroll: 3,
                    slidesToShow: 3
                }
            },
            {
                breakpoint: 500,
                settings: {
                    slidesToScroll: 2,
                    slidesToShow: 2,
                    dots: false,
                    arrows: false,
                    scrollLock: true
                }
            }
        ]
    });
    var glider = new Glider(document.getElementById('glider'));

    function sliderAuto(slider, miliseconds) {
        const slidesCount = slider.track.childElementCount;
        let slideTimeout = null;
        let nextIndex = 1;

        function slide() {
            slideTimeout = setTimeout(
                function () {
                    if (nextIndex >= slidesCount) {
                        nextIndex = 0;
                    }
                    slider.scrollItem(nextIndex++);
                },
                miliseconds
            );
        }

        slider.ele.addEventListener('glider-animated', function () {
            window.clearInterval(slideTimeout);
            slide();
        });

        slide();
    }

    sliderAuto(glider, 6000)

    $(document).ready(function () {
        if ($('.SearchQuery').val() != "" && $('.SearchQuery').val() != null && $('.SearchQuery').val() != undefined) {
            $('#showPopularFood').hide();
        }
    })
</script>
