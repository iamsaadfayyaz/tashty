@{
    string profilePhoto = "";
    if (ViewData.ContainsKey("profilePhoto"))
    {
        if (ViewData["profilePhoto"] != null)
        {
            profilePhoto = ViewData["profilePhoto"].ToString();
        }
    }
    string mealIDs = "";
    if (ViewData.ContainsKey("CartmealID"))
    {
        mealIDs = (string)ViewData["CartmealID"];
    }
    string dealIDs = "";
    if (ViewData.ContainsKey("CartdealID"))
    {
        dealIDs = (string)ViewData["CartdealID"];
    }
    int CartItemsCount = 0;
    if (ViewData.ContainsKey("CartItemsCount"))
    {
        CartItemsCount = (int)ViewData["CartItemsCount"];
    }
    bool isSeller = false;
    if (ViewData.ContainsKey("isSeller"))
    {
        isSeller = (bool)ViewData["isSeller"];
    }
    string Page = "";
    if (ViewData.ContainsKey("RedirectPage"))
    {
        Page = (string)ViewData["RedirectPage"];
    }
    bool IsAccountActive = true;
    if (ViewData.ContainsKey("IsAccountActive"))
    {
        IsAccountActive = (bool)ViewData["IsAccountActive"];
    }
}

